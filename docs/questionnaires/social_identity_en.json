{
  "title": "Social Identity Questionnaire",
  "description": "This questionnaire collects demographic information. All information will be processed anonymously.",
  "questions": [
    {
      "id": "age",
      "question": "What is your age range?",
      "type": "text",
      "hint": "e.g., 20s, 30s, or specific age"
    },
    {
      "id": "sex",
      "question": "What is your biological sex?",
      "type": "choice",
      "options": ["Male", "Female", "Other", "Prefer not to say"]
    },
    {
      "id": "gender",
      "question": "What is your gender identity?",
      "type": "choice",
      "options": ["Man", "Woman", "Non-binary", "Other", "Prefer not to say"]
    },
    {
      "id": "sexual_orientation",
      "question": "What is your sexual orientation?",
      "type": "choice",
      "options": ["Straight (heterosexual)", "Gay/Lesbian", "Bisexual", "Asexual", "Other", "Prefer not to say"]
    },
    {
      "id": "ethnicity",
      "question": "What is your ethnic background?",
      "type": "text",
      "hint": "e.g., Korean, Chinese, Japanese, Mixed, etc."
    },
    {
      "id": "race",
      "question": "What is your race?",
      "type": "choice",
      "options": ["East Asian", "Southeast Asian", "South Asian", "White", "Black", "Hispanic/Latino", "Mixed", "Other"]
    },
    {
      "id": "disability",
      "question": "Do you have any disability or health impairment?",
      "type": "choice",
      "options": ["None", "Physical disability", "Sensory disability", "Mental disability", "Multiple disabilities", "Prefer not to say"]
    },
    {
      "id": "nationality",
      "question": "What is your nationality?",
      "type": "text",
      "hint": "e.g., United States, South Korea, Japan, etc."
    },
    {
      "id": "dual_nationality",
      "question": "Do you have dual nationality?",
      "type": "choice",
      "options": ["No", "Yes (Country: _____)", "Prefer not to say"]
    },
    {
      "id": "residence",
      "question": "Where do you currently reside?",
      "type": "text",
      "hint": "e.g., Seoul, New York, etc. (specific address not needed)"
    },
    {
      "id": "education",
      "question": "What is your highest level of education?",
      "type": "choice",
      "options": ["Elementary or less", "Middle school", "High school", "Associate degree", "Bachelor's degree", "Master's degree", "Doctorate degree"]
    },
    {
      "id": "occupation",
      "question": "What is your current employment status?",
      "type": "choice",
      "options": ["Full-time employed (35+ hours/week)", "Part-time employed (<35 hours/week)", "Self-employed", "Student", "Unemployed", "Retired", "Other"]
    },
    {
      "id": "major",
      "question": "What is your field of study/major? (if applicable)",
      "type": "text",
      "hint": "e.g., Computer Science, Business, Medicine, etc."
    },
    {
      "id": "job",
      "question": "What is your specific job title? (if applicable)",
      "type": "text",
      "hint": "e.g., Software Developer, Teacher, Nurse, etc."
    },
    {
      "id": "household_income",
      "question": "What is your approximate monthly household income?",
      "type": "choice",
      "options": ["Less than $2,500", "$2,500-$5,000", "$5,000-$7,500", "$7,500-$10,000", "More than $10,000", "Prefer not to say"]
    },
    {
      "id": "perceived_income",
      "question": "How do you perceive your income level?",
      "type": "choice",
      "options": ["Below average", "Average", "Above average", "Prefer not to say"]
    },
    {
      "id": "income_satisfaction",
      "question": "How satisfied are you with your current income level?",
      "type": "choice",
      "options": ["Very dissatisfied", "Dissatisfied", "Neutral", "Satisfied", "Very satisfied"]
    },
    {
      "id": "social_class",
      "question": "Which social class do you consider yourself to belong to?",
      "type": "choice",
      "options": ["Lower class", "Lower-middle class", "Middle class", "Upper-middle class", "Upper class"]
    },
    {
      "id": "living_style",
      "question": "What is your current living arrangement?",
      "type": "choice",
      "options": ["Living alone", "Living with partner/spouse", "Living with family", "Living with roommates", "Other"]
    },
    {
      "id": "political_affiliation",
      "question": "What is your political orientation?",
      "type": "choice",
      "options": ["Very liberal", "Liberal", "Moderate", "Conservative", "Very conservative", "Prefer not to say"]
    },
    {
      "id": "religious_affiliation",
      "question": "What is your religion?",
      "type": "choice",
      "options": ["No religion", "Christianity", "Catholicism", "Buddhism", "Islam", "Other", "Prefer not to say"]
    }
  ]
}
